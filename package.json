{
  "name": "next-swatched",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "cypress": "cypress open"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "firebase": "^8.7.1",
    "next": "12.0.2",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-uuid": "^1.0.2",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "sass": "^1.35.2"
  },
  "devDependencies": {
    "@babel/preset-env": "*",
    "@babel/preset-typescript": "*",
    "@testing-library/dom": "*",
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@types/node": "*",
    "@types/react": "*",
    "babel-jest": "*",
    "cypress": "^8.7.0",
    "identity-obj-proxy": "*",
    "jest": "*",
    "jest-cli": "*",
    "jest-dom": "*",
    "start-server-and-test": "^1.14.0",
    "ts-node": "*",
    "typescript": "*"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
